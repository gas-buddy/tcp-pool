{
  "name": "@gasbuddy/tcp-pool",
  "version": "1.0.2",
  "description": "A generic pool of TCP connections with a parsing class for handling incoming and outgoing messages",
  "main": "build/index.js",
  "scripts": {
    "test": "tap --node-arg=--require --node-arg=babel-register tests/*.js",
    "test-some": "tap --node-arg=--require --node-arg=babel-register",
    "cover": "nyc --reporter=lcov npm test",
    "lint": "eslint .",
    "prepublish": "npm run build",
    "build": "babel src --out-dir build"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gas-buddy/tcp-pool.git"
  },
  "bugs": {
    "url": "https://github.com/gas-buddy/tcp-pool/issues"
  },
  "homepage": "https://github.com/gas-buddy/tcp-pool#readme",
  "keywords": [
    "tcp",
    "gasbuddy",
    "pool"
  ],
  "dependencies": {
    "bignumber.js": "^2.3.0",
    "generic-pool": "^2.4.1",
    "tap": "^7.1.2",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.2.1",
    "babel-eslint": "^7",
    "babel-plugin-syntax-async-functions": "^6.1.18",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-polyfill": "^6.2.0",
    "babel-preset-node6": "^11.0.0",
    "bluebird": "^3.3.4",
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.0.0",
    "estraverse-fb": "^1.3.1",
    "nyc": "^8.0.0",
    "pre-commit": "^1.1.2",
    "ssl-root-cas": "^1.1.10"
  },
  "author": "GasBuddy",
  "license": "MIT"
}
